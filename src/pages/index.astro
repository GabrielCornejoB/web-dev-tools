---
import Layout from "../layouts/Layout.astro";
import Card from "../components/Card.astro";
---

<Layout>
  <div class="mt-8 flex w-full flex-col items-center gap-8">
    <div class="mt-16 flex w-4/5 justify-center">
      <Card
        tool={{
          name: "Ionicons",
          author: "ionic",
          category: "other",
          description: "Open source icons. Lovingly hand-crafted.",
          url: "https://ionic.io/ionicons",
          imageURL:
            "https://firebasestorage.googleapis.com/v0/b/web-dev-tools-47601.appspot.com/o/120%2Fionicons-120px.webp?alt=media&token=998f38a5-0128-412e-96a5-3d3b6dc7bf40",
          tags: ["SVG", "IONIC"],
        }}
      />
    </div>
  </div>
</Layout>
